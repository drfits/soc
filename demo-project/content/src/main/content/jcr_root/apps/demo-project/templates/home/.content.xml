<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"

          jcr:primaryType="soc:Template"
          jcr:title="Home page"
          jcr:description="Blog home page template"
          allowedPaths="[/content/.*]">
    <jcr:content jcr:primaryType="soc:PageContent"
                 sling:resourceType="demo-project/pages/home"
                 jcr:title="Home page">
        <heading jcr:primaryType="nt:unstructured"
                 sling:resourceType="demo-project/components/heading"
                 heading="BLOG OF EVGENIY FITSNER"
                 secondary="From developer to developers about Adobe CQ/AEM, Apache Sling, Java, Magnolia CMS and other"/>
        <latest-posts jcr:primaryType="nt:unstructured"
                      sling:resourceType="demo-project/components/latest-posts"
                      size="{Long}10"/>
        <copyright jcr:primaryType="nt:unstructured"
                   sling:resourceType="demo-project/components/copyright"
                   copyright="Copyright &amp;copy; Sling Open CMS"/>
    </jcr:content>
</jcr:root>
